{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": ["**/.env"],
  "pipeline": {
    "db:generate": {
      "inputs": ["prisma/schema.prisma"],
      "cache": false
    },
    "db:push": {
      "inputs": ["prisma/schema.prisma"],
      "cache": false
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "build": {
      "dependsOn": ["^build", "^zen:generate"],
      "outputs": [".next/**", ".expo/**"]
    },
    "start": {
      "persistent": true,
      "cache": false
    },
    "lint": {},
    "lint:fix": {},
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    },
    "type-check": {
      "dependsOn": ["^zen:generate"],
      "cache": false
    },
    "zen:generate": {
      "outputs": ["**/generated/**"]
    },
    "zen:up": {},
    "zen:upgrade": {}
  },
  "globalEnv": [
    "CI",
    "DATABASE_URL",
    "DISCORD_CLIENT_ID",
    "DISCORD_CLIENT_SECRET",
    "EXPO_ROUTER_APP_ROOT",
    "NEXTAUTH_SECRET",
    "NEXTAUTH_URL",
    "NODE_ENV",
    "SKIP_ENV_VALIDATION",
    "VERCEL",
    "VERCEL_URL",
    "SWAGGER_USERNAME",
    "SWAGGER_PASSWORD"
  ]
}
